<Page x:Name="meeseeksPage" x:Class="RickButton.meeseeks"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RickButton"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="meeseeks">

    <Grid Margin="0,0,3.6,-4.918">
        <Image Source="livingroom.jpg" Stretch="Fill"/>
        <Image x:Name="portal" Margin="0,-0.082,666.837,10" Source="portal1.png" Stretch="Fill" RenderTransformOrigin="1,0.5"/>
        <Image Margin="456.157,113.947,156.606,-19.085" Source="mrMeeseeks.png" Stretch="Fill" d:IsHidden="True"/>
        <Image x:Name="rick_entering" Margin="0,10,506.855,62.799" Source="rick2.png" Stretch="Fill" d:IsHidden="True"/>
        <Image x:Name="rick_flying_out" Margin="0,22.293,470.82,46" Source="rick_flying_in.png" Stretch="Fill" FlowDirection="RightToLeft" d:IsHidden="True"/>
        <Button Cursor="Arrow" Margin="395.867,320.592,363.867,95.526">
            <Button.Template>
                <ControlTemplate>
                    <Image Source="meeseeksbox.png"/>
                </ControlTemplate>
            </Button.Template>
        </Button>
        <Image Margin="683.4,334.8,23.2,24.118" Source="portalbutton.png" Stretch="Fill"/>
    </Grid>
</Page>
